    "def check_even_list(number_list):\n",
    "    even_number = []\n",
    "    \n",
    "    for x in number_list:\n",
    "        if x % 2 == 0:\n",
    "            even_number.append(x)\n",
    "        else:\n",
    "            pass\n",
    "    return even_number"
   ]
